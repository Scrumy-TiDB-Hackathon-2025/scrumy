# Staging Environment Configuration
NEXT_PUBLIC_ENVIRONMENT=staging

# API URLs
NEXT_PUBLIC_BACKEND_URL=https://staging-api.scrumy.app
NEXT_PUBLIC_WEBSOCKET_URL=wss://staging-api.scrumy.app/ws/audio
NEXT_PUBLIC_FRONTEND_URL=https://staging.scrumy.app

# API Endpoints
NEXT_PUBLIC_API_HEALTH=/health
NEXT_PUBLIC_API_SAVE_TRANSCRIPT=/save-transcript
NEXT_PUBLIC_API_GET_MEETINGS=/get-meetings
NEXT_PUBLIC_API_GET_MODEL_CONFIG=/get-model-config
NEXT_PUBLIC_API_PROCESS_TRANSCRIPT=/process-transcript
NEXT_PUBLIC_API_TRANSCRIBE=/transcribe
NEXT_PUBLIC_API_IDENTIFY_SPEAKERS=/identify-speakers
NEXT_PUBLIC_API_GENERATE_SUMMARY=/generate-summary
NEXT_PUBLIC_API_EXTRACT_TASKS=/extract-tasks
NEXT_PUBLIC_API_PROCESS_TRANSCRIPT_WITH_TOOLS=/process-transcript-with-tools
NEXT_PUBLIC_API_AVAILABLE_TOOLS=/available-tools

# Staging Settings
NEXT_PUBLIC_DEBUG=true
NEXT_PUBLIC_MOCK_DATA=false
NEXT_PUBLIC_LOG_LEVEL=info

# Feature Flags
NEXT_PUBLIC_ENABLE_WEBSOCKET=true
NEXT_PUBLIC_ENABLE_REAL_TIME_UPDATES=true
NEXT_PUBLIC_ENABLE_MOCK_TRANSCRIPTION=false

# Performance Settings
NEXT_PUBLIC_REFRESH_INTERVAL=1500
NEXT_PUBLIC_TIMEOUT=45000

# Version
NEXT_PUBLIC_VERSION=1.0.0
